pastex <-
function (..., sep = "|", dictionary = getOption("regex.library")) {
out <- lapply(list(...), function(x) {
if (length(x) > 1) return(x)
reg_check(x, dictionary = dictionary)
})
paste(unlist(out), collapse = sep)
}
?pastex
